# Environment variables for Terraform
# Copy this file to .env and update with your values
# Source this file before running Terraform: source .env

# Hetzner Cloud API Token 
export HCLOUD_TOKEN="API_TOKEN"

# SSH Keys Configuration
# Primary SSH key (required)
export TF_VAR_ssh_public_key="ADD_YOUR_SSH_KEY"

# Secondary SSH key (set to empty to skip)
export TF_VAR_ssh_public_key_secondary=""
